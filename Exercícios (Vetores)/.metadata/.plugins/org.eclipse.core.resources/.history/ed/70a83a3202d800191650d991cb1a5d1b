import java.util.Scanner;
public class E3 {
	public static void main(String[]args) {
		Scanner ler=new Scanner(System.in);
			String v[]=new String[25];
			for(int i=0; i<25; i++) {
				System.out.println("Digite a "+(i+1)+" ª letra:");
				v[i]=ler.next();
			}
			System.out.print("(");
			for(int i=0; i<25; i++) {
				System.out.print(v[i]+", ");
			}
			System.out.print(")");	
	}
}
