import java.util.Scanner;
public class E4 {
	public static void main(String[]args) {
		Scanner ler=new Scanner(System.in);
		int v[]=new int[6];
		for(int i=0; i<6; i++) {
			System.out.println("Digite o "+(i+1)+"º valor:");
			v[i]=ler.nextInt();
		}
		for(int i=0; i<6; i++) {
			System.out.println(v[i]*2);
		}
	}
}