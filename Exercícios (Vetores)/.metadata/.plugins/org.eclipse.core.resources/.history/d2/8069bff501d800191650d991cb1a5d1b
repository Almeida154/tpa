import java.util.Scanner;
public class E3 {
	public static void main(String[]args) {
		Scanner ler=new Scanner(System.in);
			String v[]=new String[25];
			for(int i=0; i<25; i++) {
				if(i==0) {
					System.out.println("Digite a primeira letra:");
					v[i]=ler.next();
				}
				if(i==24) {
					System.out.println("Digite a última letra:");
					v[i]=ler.next();
				}
				else {
					System.out.println("Digite a próxima letra:");
					v[i]=ler.next();
				}
			}
			System.out.print("(");
			for(int i=0; i<26; i++) {
				System.out.print(v[i]+", ");
			}
			System.out.print(")");	
	}
}
